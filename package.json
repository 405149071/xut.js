{
  "name": "es6-magazine",
  "version": "1.0.0-csp",
  "description": "A es6-magazine project",
  "scripts": {
    "dev": "node build/dev/index.js",
    "build": "node build/build/index.js",
    "build:widget": "node build/build/compiler.widget.js",
    "copy": "node build/copy/index.js",
    "remote": "node build/remote/index.js",
    "lint": "eslint dist/xxtppt.dev.js",
    "tp:www": "node build/template/www.js",
    "tp:column": "node build/template/column.js",
    "tp:test": "node build/template/test.js",
    "unit": "karma start test/unit/karma.conf.js",
    "karma": "karma start karma.conf.js --log-level debug --single-run",
    "git": "bash build/shell/git.sh",
    "svn": "bash build/shell/svn.sh",
    "update:package": "node build/ncu.package.js",
    "check": "flow; test $? -eq 0 -o $? -eq 2",
    "flow:check": "flow check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JsAaron/node-magazine.git"
  },
  "author": "Aaron",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JsAaron/node-magazine/issues"
  },
  "homepage": "https://github.com/JsAaron/node-magazine#readme",
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.17.5",
    "colors": "^1.1.2",
    "eslint": "^3.9.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "flow-bin": "^0.37.4",
    "fs-extra": "^0.30.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean-css": "^2.0.13",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "gulp-watch": "^4.3.10",
    "html-webpack-plugin": "^2.24.1",
    "npm-check-updates": "^2.8.6",
    "open": "0.0.5",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-replace": "^1.1.1",
    "underscore": "^1.8.3",
    "webpack": "^1.13.3",
