    "lint": "eslint dist/xxtppt.dev.js",
    "unit": "karma start test/unit/karma.conf.js",
    "karma": "karma start karma.conf.js --log-level debug --single-run",
    "git": "bash build/shell/git.sh",
    "ncu": "node build/ncu.package.js",
    "format": "node build/format.js",
    "check": "flow; test $? -eq 0 -o $? -eq 2",
    "flow:check": "flow check"
